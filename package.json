    "happy-dom": "^12.2.1",
    "husky": "^9.1.5",
    "lint-staged": "^15.2.9",
    "typescript": "^5.0.2",
    "typescript-eslint": "^8.2.0",
    "unocss": "^0.63.2",
    "vite": "^4.4.5",
    "vite-plugin-dts": "^3.6.0",
